{
    "name": "jf-m/domain-user",
    "description": "PHP package consuming reqres API",
    "keywords": [
        "reqres"
    ],
    "homepage": "https://github.com/jf-m/domain-user",
    "license": "MIT",
    "authors": [
        {
            "name": "Jean-FranÃ§ois Meinesz",
            "email": "jfmeinesz@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "guzzlehttp/guzzle": "^7.8",
        "psr/log": "^3.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "Domain\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Domain\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
